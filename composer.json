{
    "name": "en-shop/api",
    "type": "project",
    "license": "proprietary",
    "description": "E.N Shop API",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/doctrine-orm": "^4.2.12",
        "api-platform/symfony": "^4.2.12",
        "doctrine/dbal": "^4.4.1",
        "doctrine/doctrine-bundle": ">=3.2.2",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.6.1",
        "gesdinet/jwt-refresh-token-bundle": ">=1.5",
        "lexik/jwt-authentication-bundle": ">=3.2.0",
        "nelmio/cors-bundle": ">=2.6",
        "paragonie/halite": "^5.1.4",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "ramsey/uuid": ">=4.9.2",
        "ramsey/uuid-doctrine": ">=2.1",
        "scienta/doctrine-json-functions": "^6.3",
        "stof/doctrine-extensions-bundle": ">=1.15.2",
        "stripe/stripe-php": ">=19",
        "symfony/asset": "7.4.*",
        "symfony/browser-kit": "7.4.*",
        "symfony/console": "7.4.*",
        "symfony/dotenv": "7.4.*",
        "symfony/expression-language": "7.4.*",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "7.4.*",
        "symfony/http-client": "7.4.*",
        "symfony/lock": "7.4.*",
        "symfony/mailer": "7.4.*",
        "symfony/messenger": "7.4.*",
        "symfony/monolog-bundle": ">=4.0.1",
        "symfony/property-access": "7.4.*",
        "symfony/property-info": "7.4.*",
        "symfony/rate-limiter": "7.4.*",
        "symfony/runtime": "7.4.*",
        "symfony/security-bundle": "7.4.*",
        "symfony/serializer": "7.4.*",
        "symfony/translation": "7.4.*",
        "symfony/twig-bundle": "7.4.*",
        "symfony/validator": "7.4.*",
        "symfony/yaml": "7.4.*",
        "symfony/amqp-messenger": "7.4.*",
        "symfony/doctrine-messenger": "7.4.*",
        "twig/cssinliner-extra": "^3.22",
        "twig/extra-bundle": "^3.22.2",
        "vich/uploader-bundle": ">=2.9.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": ">=8.4.1",
        "doctrine/doctrine-fixtures-bundle": "^4.3.1",
        "fakerphp/faker": "^1.24.1",
        "friendsofphp/php-cs-fixer": "^3.92.5",
        "mikey179/vfsstream": "^1.6.12",
        "phpmd/phpmd": "^2.15",
        "phpro/grumphp": "^2.18",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpdoc-parser": "^2.3",
        "phpstan/phpstan": "^2.1.33",
        "phpunit/phpunit": "12.4.4",
        "rector/rector": "^2.3.0",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^4.0.1",
        "symfony/debug-bundle": "7.4.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/web-profiler-bundle": "7.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true,
            "phpro/grumphp": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Application\\": "application/src/",
            "App\\Domain\\Shop\\": "domain/Shop/src/",
            "App\\Domain\\SharedKernel\\": "domain/SharedKernel/src/",
            "App\\Domain\\User\\": "domain/User/src/",
            "App\\Infrastructure\\": "infrastructure/src/",
            "App\\Presentation\\": "presentation/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Application\\Tests\\": "application/tests/",
            "App\\Domain\\Shop\\Tests\\": "domain/Shop/tests/",
            "App\\Domain\\User\\Tests\\": "domain/User/tests/",
            "App\\Infrastructure\\Tests\\": "infrastructure/tests/",
            "App\\Presentation\\Tests\\": "presentation/tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    }
}
